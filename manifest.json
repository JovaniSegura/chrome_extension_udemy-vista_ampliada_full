{
  "manifest_version": 3,
  "name": "Udemy VH Expander",
  "version": "1.0",
  "description": "Expande la vista de los cursos de Udemy",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.udemy.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
